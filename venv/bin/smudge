#!/Users/tomhill/Projects/Sideprojects/blot-cli/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'Smudge','console_scripts','smudge'
__requires__ = 'Smudge'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('Smudge', 'console_scripts', 'smudge')()
    )
